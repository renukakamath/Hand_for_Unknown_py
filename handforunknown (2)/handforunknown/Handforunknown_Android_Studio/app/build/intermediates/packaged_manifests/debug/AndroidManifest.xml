            android:label="@string/title_activity_registration" />
        <activity
            android:name="com.example.handforunknown.ParentHome"
            android:label="@string/title_activity_parent_home" />
        <activity
            android:name="com.example.handforunknown.IPSetting"
            android:exported="true"
            android:label="@string/title_activity_ipsetting" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.handforunknown.Parentaddchild"
            android:label="@string/title_activity_parentaddchild" />
